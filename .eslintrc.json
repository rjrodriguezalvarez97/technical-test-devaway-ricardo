{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["prettier", "airbnb-base"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-console": "off",
    "function-paren-newline": "off",
    "implicit-arrow-linebreak": "off",
    "comma-dangle": [
      "error",
      {
        "functions": "never"
      }
    ]
  }
}
